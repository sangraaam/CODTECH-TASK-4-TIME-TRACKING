{
    "manifest_version": 3,
    "name": "Web Time Tracker",
    "version": "1.0",
    "description": "Tracks time spent on websites.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icon128.png"
      }
    },
    "icons": {
      "128": "icon128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ]
  }
  
  
  